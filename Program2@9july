import java.util.*;
class Length
{
public static void main(String args[])
{
int sum=0;
Scanner sc=new Scanner(System.in);
String st;
System.out.println("Enter string : ");
st=sc.nextLine();
String ch[]=st.split(" ");
int n=ch.length;
if(n==0)
System.out.println(n);
else
{
for(int i=0;i<n;i++)
{
String str=ch[i];
int m=str.length();
sum=sum+m;
}
System.out.println(sum);
}
}
}
