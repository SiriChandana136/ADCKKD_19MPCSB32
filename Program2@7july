import java.util.*;
class PrimeCount
{
public static void main(String args[])
{
int c,p=0,i,j,n;
Scanner s=new Scanner(System.in);
n=s.nextInt();
int a[]=new int[n];
System.out.println("Enter array elements");
for(i=0;i<n;i++)

a[i]=s.nextInt();
for(i=0;i<n;i++)

{
c=0;
for(j=2;j<a[i];j++)

{
if(a[i]%j==0)
c++;
}
if(c==0)
p++;
}
System.out.println("Number of Primes "+p);
}
}
