import java.util.*;
class Min
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int n,i;
System.out.print("Enter array size");
n=s.nextInt();
int a[]=new int[n];
System.out.println("Enter array elements");
for(i=0;i<n;i++)
a[i]=s.nextInt();
Arrays.sort(a);
System.out.println("Minimum element is "+a[0]);
}
}
